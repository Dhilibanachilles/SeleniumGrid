<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
	<test name="test1" thread-count="5">
	<parameter name="bname" value="chrome"></parameter>
		<classes>
			<class name="com.starAgile.SeleniumGrid.ParallelExecution"></class>
		</classes>
	</test> <!-- Test -->
	<test name="Test2" thread-count="5">
	<parameter name="bname" value="firefox"></parameter>
		<classes>
			<class name="com.starAgile.SeleniumGrid.ParallelExecution"></class>
		</classes>
	</test> <!-- Test -->
	<test name="Test3" thread-count="5">
	<parameter name="bname" value="edge"></parameter>
		<classes>
			<class name="com.starAgile.SeleniumGrid.ParallelExecution"></class>
		</classes>
	</test> <!-- Test -->
</suite>

